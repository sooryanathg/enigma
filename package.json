    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "nodemon": "^3.1.10",
    "prettier": "^3.7.4",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-declaration-strict-value": "^1.10.11",
    "ts-node": "^10.9.2",
